{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "es5",
        "removeComments": false,
        "sourceMap": true,
        "inlineSources": true,
        "noImplicitThis": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "lib": [
            "dom",
            "es5",
            "es2015.core",
            "es2015.promise"
        ]
    },
    "exclude": [
        "node_modules"
    ]
}
